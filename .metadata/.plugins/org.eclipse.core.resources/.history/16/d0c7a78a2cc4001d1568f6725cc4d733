package DAO;

import javax.swing.JButton;
import javax.swing.JOptionPane;

import Dominio.Login;

public class LoginDAO {
	
	public void autenticar(Login l, JButton btn) {
		if(l.getUser().equals("admin") && l.getPass().equals("admin")) {
			JOptionPane.showMessageDialog(btn, "Autenticação bem sucedida!");
		}else {
			JOptionPane.showMessageDialog(btn, "Erro na autenticação!");
		}
	}

}
