		lblNewLabel_5.setBounds(25, 160, 46, 14);
		panel.add(lblNewLabel_5);
		
		textField = new JTextField();
		textField.setBounds(25, 73, 374, 20);
		panel.add(textField);
		textField.setColumns(10);
		
